{
  "name": "sparqljs",
  "version": "1.5.2",
  "description": "A parser for the SPARQL query language",
  "author": "Ruben Verborgh <ruben.verborgh@gmail.com>",
  "keywords": [
    "sparql",
    "rdf",
    "query",
    "parser"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RubenVerborgh/SPARQL.js.git"
  },
  "bugs": {
    "url": "https://github.com/RubenVerborgh/SPARQL.js/issues"
  },
  "main": "sparql.js",
  "bin": "bin/sparql-to-json",
  "scripts": {
    "build": "jison lib/sparql.jison -p slr -o lib/SparqlParser.js",
    "test": "mocha"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "devDependencies": {
    "jison": "0.4.18",
    "mocha": "4.0.x",
    "chai": "4.1.x",
    "pre-commit": "~1.2.2"
  },
  "browser": {
    "_process": false,
    "fs": false,
    "path": false
  },
  "pre-commit": [
    "build",
    "test"
  ],
  "testling": {
    "files": "test/*.js",
    "harness": "mocha",
    "browsers": [
      "ie/9..latest",
      "firefox/24..latest",
      "chrome/29..latest",
      "safari/6..latest",
      "iphone/6..latest",
      "ipad/6..latest"
    ]
  }
}
